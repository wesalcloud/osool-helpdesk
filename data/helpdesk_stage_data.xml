<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Helpdesk Stages -->
    <record id="stage_new" model="helpdesk.stage">
        <field name="name">New</field>
        <field name="sequence">1</field>
        <field name="is_new" eval="True"/>
        <field name="fold" eval="False"/>
    </record>

    <record id="stage_assigned" model="helpdesk.stage">
        <field name="name">Assigned</field>
        <field name="sequence">2</field>
        <field name="is_assigned" eval="True"/>
        <field name="fold" eval="False"/>
    </record>

    <record id="stage_in_progress" model="helpdesk.stage">
        <field name="name">In Progress</field>
        <field name="sequence">3</field>
        <field name="is_in_progress" eval="True"/>
        <field name="fold" eval="False"/>
    </record>

    <record id="stage_pending_internal" model="helpdesk.stage">
        <field name="name">Pending Internal</field>
        <field name="sequence">4</field>
        <field name="fold" eval="False"/>
    </record>

    <record id="stage_pending_customer" model="helpdesk.stage">
        <field name="name">Pending Customer</field>
        <field name="sequence">5</field>
        <field name="fold" eval="False"/>
    </record>

    <record id="stage_resolved" model="helpdesk.stage">
        <field name="name">Resolved</field>
        <field name="sequence">6</field>
        <field name="is_resolved" eval="True"/>
        <field name="fold" eval="False"/>
    </record>

    <record id="stage_closed" model="helpdesk.stage">
        <field name="name">Closed</field>
        <field name="sequence">7</field>
        <field name="is_closed" eval="True"/>
        <field name="fold" eval="True"/>
    </record>

    <record id="stage_rejected" model="helpdesk.stage">
        <field name="name">Rejected</field>
        <field name="sequence">8</field>
        <field name="is_rejected" eval="True"/>
        <field name="fold" eval="True"/>
    </record>

    <record id="stage_cancelled" model="helpdesk.stage">
        <field name="name">Cancelled</field>
        <field name="sequence">9</field>
        <field name="fold" eval="True"/>
    </record>

</odoo>
